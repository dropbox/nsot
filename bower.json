{
  "name": "nsot",
  "authors": [
    "Gary M. Josack <gary@dropbox.com>"
  ],
  "description": "NSoT is a Network Source of Truth API and Frontend for managing Network Assets.",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "vendor",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "1.3.15",
    "angular-route": "1.3.15",
    "angular-resource": "1.3.15",
    "bootstrap": "3.3.2",
    "font-awesome": "4.3.0",
    "lodash": "3.7.0",
    "ng-tags-input": "2.3.0",
    "moment": "2.10.2",
    "jquery": "2.1.4",
    "angular-chart.js": "~0.8.5",
    "restangular": "~1.5.1"
  },
  "overrides": {
    "moment": {
      "main": [
        "min/moment.min.js"
      ]
    },
    "ng-tags-input": {
      "main": [
        "ng-tags-input.min.*",
        "ng-tags-input.*.min.*"
      ]
    },
    "lodash": {
      "main": [
        "lodash.min.js"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "angular-resource": {
      "main": [
        "angular-resource.min.js",
        "angular-resource.min.js.map"
      ]
    },
    "angular-route": {
      "main": [
        "angular-route.min.js",
        "angular-route.min.js.map"
      ]
    },
    "angular": {
      "main": [
        "angular.min.js",
        "angular.min.js.map"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js"
      ]
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    }
  }
}
